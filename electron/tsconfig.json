{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "../build",
    "rootDir": "../",
    "strictPropertyInitialization": false,
    "noEmitOnError": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
